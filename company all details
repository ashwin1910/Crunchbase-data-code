                    investment_round_updated_at_investment_round_investor.append(investment_round['properties']['updated_at'])
                    # Investor details
                    investment_round_investor_money_invested.append(investment_round_investors['properties']['money_invested'])
                    investment_round_investor_money_invested_currency_code.append(investment_round_investors['properties']['money_invested_currency_code'])
                    investment_round_investor_money_invested_usd.append(investment_round_investors['properties']['money_invested_usd'])
                    investment_round_investor_is_lead_investor.append(investment_round_investors['properties']['is_lead_investor'])
                    investment_round_investor_type.append(investment_round_investors['relationships']['investors']['type'])
                    investment_round_investor_uuid.append(investment_round_investors['relationships']['investors']['uuid'])
                    investment_round_investor_permalink.append(investment_round_investors['relationships']['investors']['properties']['permalink'])
                    investment_round_investor_permalink_aliases.append(investment_round_investors['relationships']['investors']['properties']['permalink_aliases'])
                    investment_round_investor_api_path.append(investment_round_investors['relationships']['investors']['properties']['api_path'])
                    investment_round_investor_web_path.append(investment_round_investors['relationships']['investors']['properties']['web_path'])
                    investment_round_investor_api_url.append(investment_round_investors['relationships']['investors']['properties']['api_url'])
                    investment_round_investor_rank.append(investment_round_investors['relationships']['investors']['properties']['rank'])
                    investment_round_investor_created_at.append(investment_round_investors['relationships']['investors']['properties']['created_at'])
                    investment_round_investor_updated_at.append(investment_round_investors['relationships']['investors']['properties']['updated_at'])
                    investment_round_investor_profile_image_url.append(investment_round_investors['relationships']['investors']['properties']['profile_image_url'])
                    
                    if investment_round_investor_type[-1] == 'Organization': #Different details depending upon whether the investor is organization or person             
                        investment_round_investor_name.append(investment_round_investors['relationships']['investors']['properties']['name'])
