                # All investments made by each featured_member
                featured_member_investments_paging_total_items.append(featured_member_details['investments']['paging']['total_items'])
                if featured_member_investments_paging_total_items[-1]>0: # If featured_member website details are available, append the details
                    page=1
                    
                    featured_member_investments_url = featured_member_details['investments']['paging']['first_page_url']+'?user_key={}'.format(api_key)
                    response = urllib.request.urlopen(featured_member_investments_url, timeout=timeout_time_seconds)
                    data_featured_member_investments = json.loads(response.read().decode())
                    featured_member_investments_paging_number_of_pages= data_featured_member_investments['data']['paging']['number_of_pages']

                    
                    while page <= featured_member_investments_paging_number_of_pages: # Pagination - For any item, CrunchBase API shows at most 100 entries per page. For items with over 100 entries, we have to scrape details from each page
                        featured_member_investments_url = featured_member_details['investments']['paging']['first_page_url'] + '?page={}&sort_order=announced_on%20DESC&items_per_page=100&user_key={}'.format(page, api_key)
                        
                        try:
                            response = urllib.request.urlopen(featured_member_investments_url, timeout=timeout_time_seconds)
                        except:
                            #time_excep_featured_team_investment.append(url)
                            continue
                        data_featured_member_investments = json.loads(response.read().decode())
                        page = page+1
                        for featured_member_investments in data_featured_member_investments['data']['items']:
                            
                            company_uuid_featured_member_investments.append(company_details['uuid'])
                            company_name_featured_member_investments.append(company_details['properties']['name'])
