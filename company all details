                    featured_member_founded_company_updated_at.append(featured_member_founded_companies['properties']['updated_at'])
                 
                # All website urls (Facebook, Twitter, and LinkedIn) for each featured_member
                featured_member_website_paging_total_items.append(featured_member_details['websites']['paging']['total_items'])
                if featured_member_website_paging_total_items[-1]>0: # If featured_member website details are available, append the details
                    
                    for featured_member_websites in data_featured_member['data']['relationships']['websites']['items']:
                    
                        company_uuid_featured_member_website.append(company_details['uuid'])
                        company_name_featured_member_website.append(company_details['properties']['name'])
                        featured_member_uuid_featured_member_website.append(featured_team_details['relationships']['person']['uuid'])
