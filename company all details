                    response = urllib.request.urlopen(past_member_investments_url, timeout=timeout_time_seconds)
                    data_past_member_investments = json.loads(response.read().decode())
                    past_member_investments_paging_number_of_pages= data_past_member_investments['data']['paging']['number_of_pages']

                    
