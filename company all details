                            featured_member_investment_funding_round_funded_organization_homepage_url.append(featured_member_investments['relationships']['funding_round']['relationships']['funded_organization']['properties']['homepage_url'])
                            featured_member_investment_funding_round_funded_organization_contact_email.append(featured_member_investments['relationships']['funding_round']['relationships']['funded_organization']['properties']['contact_email'])
                            featured_member_investment_funding_round_funded_organization_phone_number.append(featured_member_investments['relationships']['funding_round']['relationships']['funded_organization']['properties']['phone_number'])
                            featured_member_investment_funding_round_funded_organization_rank.append(featured_member_investments['relationships']['funding_round']['relationships']['funded_organization']['properties']['rank'])
                            featured_member_investment_funding_round_funded_organization_created_at.append(featured_member_investments['relationships']['funding_round']['relationships']['funded_organization']['properties']['created_at'])
                            featured_member_investment_funding_round_funded_organization_updated_at.append(featured_member_investments['relationships']['funding_round']['relationships']['funded_organization']['properties']['updated_at'])
    
                                
    
    
    
        #extracting current member
        current_team_url = company_details['relationships']['current_team']['paging']\
        ['first_page_url'] +'?user_key={}'.format(api_key)
        
        response = urllib.request.urlopen(current_team_url, timeout=timeout_time_seconds)
        data_current_team = json.loads(response.read().decode())
        current_team_paging_number_of_pages= data_current_team['data']['paging']['number_of_pages']
        page_current_team=1
        while page_current_team <= current_team_paging_number_of_pages: # Pagination - For any item, CrunchBase API shows at most 100 entries per page. For items with over 100 entries, we have to scrape details from each page
            current_team_url = company_details['relationships']['current_team']['paging']['first_page_url'] + '?page={}%20DESC&items_per_page=100&user_key={}'.format(page_current_team, api_key)
            

            response = urllib.request.urlopen(current_team_url, timeout=timeout_time_seconds)
            data_current_team = json.loads(response.read().decode())
            page_current_team = page_current_team+1        
            for current_team_details in data_current_team['data']['items']:
                company_uuid_current_member.append(company_details['uuid'])
                company_name_current_member.append(company_details['properties']['name'])
                current_member_type.append(current_team_details['type'])
                current_member_uuid.append(current_team_details['relationships']['person']['uuid'])
                current_member_permalink.append(current_team_details['relationships']['person']['properties']['permalink'])
                current_member_api_path.append(current_team_details['relationships']['person']['properties']['api_path'])
                current_member_web_path.append(current_team_details['relationships']['person']['properties']['web_path'])
                current_member_api_url.append(current_team_details['relationships']['person']['properties']['api_url'])
                current_member_first_name.append(current_team_details['relationships']['person']['properties']['first_name'])
                current_member_last_name.append(current_team_details['relationships']['person']['properties']['last_name'])
                current_member_gender.append(current_team_details['relationships']['person']['properties']['gender'])
                current_member_also_known_as.append(current_team_details['relationships']['person']['properties']['also_known_as'])
                current_member_bio.append(current_team_details['relationships']['person']['properties']['bio'])
                current_member_profile_image_url.append(current_team_details['relationships']['person']['properties']['profile_image_url'])
                current_member_role_investor.append(current_team_details['relationships']['person']['properties']['role_investor'])
                current_member_born_on.append(current_team_details['relationships']['person']['properties']['born_on'])
                current_member_born_on_trust_code.append(current_team_details['relationships']['person']['properties']['born_on_trust_code'])
                current_member_died_on.append(current_team_details['relationships']['person']['properties']['died_on'])
                current_member_died_on_trust_code.append(current_team_details['relationships']['person']['properties']['died_on_trust_code'])
                current_member_rank.append(current_team_details['relationships']['person']['properties']['rank'])
                current_member_created_at.append(current_team_details['relationships']['person']['properties']['created_at'])
                current_member_updated_at.append(current_team_details['relationships']['person']['properties']['updated_at'])    
    
                current_member_url= 'https://api.crunchbase.com/v3.1/people/{}?user_key={}'.format(current_team_details['relationships']['person']['uuid'], api_key)
                

                response = urllib.request.urlopen(current_member_url, timeout=timeout_time_seconds)
                data_current_member = json.loads(response.read().decode())
                current_member_details = data_current_member['data']['relationships']
                # Primary locations for each current_member
                current_member_primary_location_total_items.append(current_member_details['primary_location']['paging']['total_items'])
                if current_member_primary_location_total_items[-1] >0: # If current_member location details are available, append the details
                    current_member_primary_location_type.append(current_member_details['primary_location']['item']['type'])
                    current_member_primary_location_uuid.append(current_member_details['primary_location']['item']['uuid'])
                    current_member_primary_location_location_type.append(current_member_details['primary_location']['item']['properties']['location_type'])
