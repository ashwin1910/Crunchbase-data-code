                            member_investment_funding_round_funded_organization_is_closed.append(member_investments['relationships']['funding_round']['relationships']['funded_organization']['properties']['is_closed'])
                            member_investment_funding_round_funded_organization_closed_on.append(member_investments['relationships']['funding_round']['relationships']['funded_organization']['properties']['closed_on'])
                            member_investment_funding_round_funded_organization_closed_on_trust_code.append(member_investments['relationships']['funding_round']['relationships']['funded_organization']['properties']['closed_on_trust_code'])
                            member_investment_funding_round_funded_organization_num_employees_min.append(member_investments['relationships']['funding_round']['relationships']['funded_organization']['properties']['num_employees_min'])
                            member_investment_funding_round_funded_organization_num_employees_max.append(member_investments['relationships']['funding_round']['relationships']['funded_organization']['properties']['num_employees_max'])
                            member_investment_funding_round_funded_organization_stock_exchange.append(member_investments['relationships']['funding_round']['relationships']['funded_organization']['properties']['stock_exchange'])
                            member_investment_funding_round_funded_organization_stock_symbol.append(member_investments['relationships']['funding_round']['relationships']['funded_organization']['properties']['stock_symbol'])
                            member_investment_funding_round_funded_organization_total_funding_usd.append(member_investments['relationships']['funding_round']['relationships']['funded_organization']['properties']['total_funding_usd'])
                            member_investment_funding_round_funded_organization_number_of_investments.append(member_investments['relationships']['funding_round']['relationships']['funded_organization']['properties']['number_of_investments'])
                            member_investment_funding_round_funded_organization_homepage_url.append(member_investments['relationships']['funding_round']['relationships']['funded_organization']['properties']['homepage_url'])
                            member_investment_funding_round_funded_organization_contact_email.append(member_investments['relationships']['funding_round']['relationships']['funded_organization']['properties']['contact_email'])
                            member_investment_funding_round_funded_organization_phone_number.append(member_investments['relationships']['funding_round']['relationships']['funded_organization']['properties']['phone_number'])
                            member_investment_funding_round_funded_organization_rank.append(member_investments['relationships']['funding_round']['relationships']['funded_organization']['properties']['rank'])
                            member_investment_funding_round_funded_organization_created_at.append(member_investments['relationships']['funding_round']['relationships']['funded_organization']['properties']['created_at'])
                            member_investment_funding_round_funded_organization_updated_at.append(member_investments['relationships']['funding_round']['relationships']['funded_organization']['properties']['updated_at'])

                                
        
        
        # Re-declaring all the ouput data frames after every company is scraped
        Investors= pd.DataFrame()
        Company_Funds= pd.DataFrame()
        Company_Investments = pd.DataFrame()
