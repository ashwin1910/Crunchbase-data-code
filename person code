#going through each person in for loop
for person_permalink in person_permalink_list:
    try:
        person_url= 'https://api.crunchbase.com/v3.1/people/{}?user_key={}'.format(person_permalink, api_key)
        response = urllib.request.urlopen(person_url, timeout=timeout_time_seconds)
        data_person = json.loads(response.read().decode())
        person_properties_details=data_person['data']
        person_details = data_person['data']['relationships']  
        
        #collecting person properties
        person_type.append(person_properties_details['type'])
        person_uuid.append(person_properties_details['uuid'])
        person_permalink.append(person_properties_details['properties']['permalink'])
        person_api_path.append(person_properties_details['properties']['api_path'])
        person_web_path.append(person_properties_details['properties']['web_path'])
        person_api_url.append(person_properties_details['properties']['api_url'])
        person_first_name.append(person_properties_details['properties']['first_name'])
        person_last_name.append(person_properties_details['properties']['last_name'])
        person_gender.append(person_properties_details['properties']['gender'])
        person_also_known_as.append(person_properties_details['properties']['also_known_as'])
        person_bio.append(person_properties_details['properties']['bio'])
        person_profile_image_url.append(person_properties_details['properties']['profile_image_url'])
        person_role_investor.append(person_properties_details['properties']['role_investor'])
        person_born_on.append(person_properties_details['properties']['born_on'])
        person_born_on_trust_code.append(person_properties_details['properties']['born_on_trust_code'])
        person_died_on.append(person_properties_details['properties']['died_on'])
        person_died_on_trust_code.append(person_properties_details['properties']['died_on_trust_code'])
        person_rank.append(person_properties_details['properties']['rank'])
        person_created_at.append(person_properties_details['properties']['created_at'])
        person_updated_at.append(person_properties_details['properties']['updated_at'])
        
        
        
        # Primary locations for each person
        person_primary_location_total_items.append(person_details['primary_location']['paging']['total_items'])
        if person_primary_location_total_items[-1] >0: # If person location details are available, append the details
            person_primary_location_type.append(person_details['primary_location']['item']['type'])
            person_primary_location_uuid.append(person_details['primary_location']['item']['uuid'])
            person_primary_location_location_type.append(person_details['primary_location']['item']['properties']['location_type'])
            person_primary_location_parent_location_uuid.append(person_details['primary_location']['item']['properties']['parent_location_uuid'])
            person_primary_location_city.append(person_details['primary_location']['item']['properties']['web_path'])
            person_primary_location_region.append(person_details['primary_location']['item']['properties']['region'])
            person_primary_location_region_code2.append(person_details['primary_location']['item']['properties']['region_code2'])
            person_primary_location_country.append(person_details['primary_location']['item']['properties']['country'])
            person_primary_location_country_code2.append(person_details['primary_location']['item']['properties']['country_code2'])
            person_primary_location_country_code3.append(person_details['primary_location']['item']['properties']['country_code3'])
            person_primary_location_continent.append(person_details['primary_location']['item']['properties']['continent'])
            person_primary_location_created_at.append(person_details['primary_location']['item']['properties']['created_at'])
            person_primary_location_updated_at.append(person_details['primary_location']['item']['properties']['updated_at'])
        else: # Else append as missing entries
            person_primary_location_type.append('')
            person_primary_location_uuid.append('')
            person_primary_location_location_type.append('')
            person_primary_location_parent_location_uuid.append('')
            person_primary_location_city.append('')
            person_primary_location_region.append('')
            person_primary_location_region_code2.append('')
            person_primary_location_country.append('')
            person_primary_location_country_code2.append('')
            person_primary_location_country_code3.append('')
            person_primary_location_continent.append('')
            person_primary_location_created_at.append('')
            person_primary_location_updated_at.append('')            
            
        # Primary affiliations for each person
        person_primary_affiliation_total_items.append(person_details['primary_affiliation']['paging']['total_items'])
        if person_primary_affiliation_total_items[-1] >0: # If person affiliation details are available, append the details
            person_primary_affiliation_type.append(person_details['primary_affiliation']['item']['type'])
            person_primary_affiliation_uuid.append(person_details['primary_affiliation']['item']['uuid'])
            person_primary_affiliation_title.append(person_details['primary_affiliation']['item']['properties']['title'])
            person_primary_affiliation_started_on.append(person_details['primary_affiliation']['item']['properties']['started_on'])
            person_primary_affiliation_started_on_trust_code.append(person_details['primary_affiliation']['item']['properties']['started_on_trust_code'])
            person_primary_affiliation_ended_on.append(person_details['primary_affiliation']['item']['properties']['ended_on'])
            person_primary_affiliation_ended_on_trust_code.append(person_details['primary_affiliation']['item']['properties']['ended_on_trust_code'])            
            person_primary_affiliation_is_current.append(person_details['primary_affiliation']['item']['properties']['is_current'])
            person_primary_affiliation_job_type.append(person_details['primary_affiliation']['item']['properties']['job_type'])
            person_primary_affiliation_created_at.append(person_details['primary_affiliation']['item']['properties']['created_at'])
            person_primary_affiliation_updated_at.append(person_details['primary_affiliation']['item']['properties']['updated_at'])
            person_primary_affiliation_organization_type.append(person_details['primary_affiliation']['item']['relationships']['organization']['type'])
            person_primary_affiliation_organization_uuid.append(person_details['primary_affiliation']['item']['relationships']['organization']['uuid'])
            person_primary_affiliation_organization_permalink.append(person_details['primary_affiliation']['item']['relationships']['organization']['properties']['permalink'])
            person_primary_affiliation_organization_permalink_aliases.append(person_details['primary_affiliation']['item']['relationships']['organization']['properties']['permalink_aliases'])
            person_primary_affiliation_organization_api_path.append(person_details['primary_affiliation']['item']['relationships']['organization']['properties']['api_path'])
            person_primary_affiliation_organization_web_path.append(person_details['primary_affiliation']['item']['relationships']['organization']['properties']['web_path'])
            person_primary_affiliation_organization_api_url.append(person_details['primary_affiliation']['item']['relationships']['organization']['properties']['api_url'])
            person_primary_affiliation_organization_name.append(person_details['primary_affiliation']['item']['relationships']['organization']['properties']['name'])
            person_primary_affiliation_organization_also_known_as.append(person_details['primary_affiliation']['item']['relationships']['organization']['properties']['also_known_as'])
            person_primary_affiliation_organization_short_description.append(person_details['primary_affiliation']['item']['relationships']['organization']['properties']['short_description'])
            person_primary_affiliation_organization_description.append(person_details['primary_affiliation']['item']['relationships']['organization']['properties']['description'])
            person_primary_affiliation_organization_profile_image_url.append(person_details['primary_affiliation']['item']['relationships']['organization']['properties']['profile_image_url'])
