        Person_AdvisoryRoles.to_csv(r'Person_AdvisoryRoles_'+ folder_location_name +'.csv')
        Person_Investments.to_csv(r'Person_Investments_'+ folder_location_name +'.csv')
    except:
     
        Error=pd.DataFrame()
        person_url_error.append(person_permalink)
        Error['company_url']=person_url_error
        Error.to_csv(r'Error_'+ folder_location_name +'.csv')
        print(person_permalink)
        print('\n')
        time.sleep(180)
        continue
