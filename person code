        #writing all dataframes to csv
        Person.to_csv(r'Person_'+ folder_location_name +'.csv')
        Person_Degrees.to_csv(r'Person_Degrees_'+ folder_location_name +'.csv')
        Person_Jobs.to_csv(r'Person_Jobs_'+ folder_location_name +'.csv')
        Person_FoundedCompanies.to_csv(r'Person_FoundedCompanies_'+ folder_location_name +'.csv')
