                            current_member_investment_money_invested_usd.append(current_member_investments['properties']['money_invested_usd'])
                            current_member_investment_is_lead_investor.append(current_member_investments['properties']['is_lead_investor'])
                            current_member_investment_announced_on.append(current_member_investments['properties']['announced_on'])
                            current_member_investment_announced_on_trust_code.append(current_member_investments['properties']['announced_on_trust_code'])
                            current_member_investment_created_at.append(current_member_investments['properties']['created_at'])
