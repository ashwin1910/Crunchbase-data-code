# Dataframes for companies skipped because of timeout error
timeout_excep_company =[]
timeout_excep_funding_company=[]
timeout_excep_fundings=[]
timeout_excep_investors=[]
timeout_exceptions_acquistions=[]
timeout_except_acquisition=[]
timeout_except_offices=[]
timeout_except_office=[]
timeout_excep_headquarters=[]
timeout_excep_headquarter=[]
time_excep_suborganisations=[]
time_excep_suborganisation=[]
time_excep_company_founders=[]
time_excep_company_founder=[]
timeout_excep_founder_details=[]
time_excep_founders_investment=[]



funding_api_url_check=[]
company_details=[]

index=start_index
timeout_time_seconds= 100 #User-defined timeout timeframe

company_count= 1 # Company Counter for tracking
company_count_error= 0 # Company Error Counter for tracking companies returning error while scraping
# Basic details for each company from each company's CrunchBase API url
for company_url in company_url_list:
    #tracking and printing index
    company_start= time.time()
    
    #tracking index
    print(('current index is {}').format(index))
    index= index+1

    
    try:
        #this part of code scrape details of a company if the company shows investment 
        url= company_url
        response = urllib.request.urlopen(url, timeout=timeout_time_seconds)
        data = json.loads(response.read().decode())
        company_details= data['data']
        uuid = data['data']['uuid']
        investor_uuid.append(uuid)        
        permalink = data['data']['properties']['permalink']
        investor_permalink.append(permalink)
        investor_permalink_aliases.append(data['data']['properties']['permalink_aliases'])
        investor_api_path.append(data['data']['properties']['api_path'])
        investor_web_path.append(data['data']['properties']['web_path'])
        investor_api_url.append(data['data']['properties']['api_url'])
        name = data['data']['properties']['name']
        investor_name.append(name)
     
        investor_also_known_as.append(data['data']['properties']['also_known_as'])
        investor_short_description.append(data['data']['properties']['short_description'])
        investor_description.append(data['data']['properties']['description'])
        investor_investor_type.append(data['data']['properties']['investor_type'])
        investor_founded_on.append(data['data']['properties']['founded_on'])
        investor_closed_on.append(data['data']['properties']['closed_on'])
        investor_num_employees_min.append(data['data']['properties']['num_employees_min'])
        investor_num_employees_max.append(data['data']['properties']['num_employees_max'])
        investor_stock_exchange.append(data['data']['properties']['stock_exchange'])
        investor_stock_symbol.append(data['data']['properties']['stock_symbol'])
        investor_total_funding_usd.append(data['data']['properties']['total_funding_usd'])
        investor_number_of_investments.append(data['data']['properties']['number_of_investments'])
        investor_homepage_url.append(data['data']['properties']['homepage_url'])        
        
        permalink = data['data']['properties']['permalink']
        permalink = data['data']['properties']['permalink']
        url = r'https://api.crunchbase.com/v3.1/organizations/{}/funds?user_key={}'\
                .format(permalink, api_key)
        response = urllib.request.urlopen(url)
        data = json.loads(response.read().decode())
        
        
        for key in data['data']['items']:
    
            funds_paging_total_items.append(data['data']['paging']['total_items'])
            funds_paging_number_of_pages.append(data['data']['paging']['number_of_pages'])
            funds_paging_current_page.append(data['data']['paging']['current_page'])
            funds_paging_sort_order.append(data['data']['paging']['sort_order'])
            funds_paging_items_per_page.append(data['data']['paging']['items_per_page'])
            funds_paging_next_page_url.append(data['data']['paging']['next_page_url'])
            funds_paging_prev_page_url.append(data['data']['paging']['prev_page_url'])
            funds_paging_key_set_url.append(data['data']['paging']['key_set_url'])
            funds_paging_collection_url.append(data['data']['paging']['collection_url'])
            
            # fund particulars
            fund_investor.append(name)
            fund_investor_uuid.append(uuid)        
            fund_type.append(key['type'])
            fund_uuid.append(uuid)
            fund_permalink.append(key['properties']['permalink'])
            fund_api_path.append(key['properties']['api_path'])
            fund_web_path.append(key['properties']['web_path'])
            fund_api_url.append(key['properties']['api_url'])
            fund_name.append(key['properties']['name'])
            fund_announced_on.append(key['properties']['announced_on'])
            fund_money_raised.append(key['properties']['money_raised'])
            fund_money_raised_currency_code.append(key['properties']['money_raised_currency_code'])
            fund_money_raised_usd.append(key['properties']['money_raised_usd'])
            
                
        # Investment Details
        url= company_url
        try:
            response = urllib.request.urlopen(url, timeout=timeout_time_seconds)
        except:
            timeout_excep_company.append(url)
            time.sleep(2)
            continue
        data_x = json.loads(response.read().decode())
        company_details= data_x['data']    
        
        url = r'https://api.crunchbase.com/v3.1/organizations/{}/investments?user_key={}'\
                .format(permalink, api_key)
        
        try:
            response = urllib.request.urlopen(url, timeout=timeout_time_seconds)
        except:
            continue
        data = json.loads(response.read().decode())
        investments_paging_number_of_pages= data['data']['paging']['number_of_pages']
        page_investments=1
        while page_investments <= investments_paging_number_of_pages: # Pagination - For any item, CrunchBase API shows at most 100 entries per page. For items with over 100 entries, we have to scrape details from each page
            url = company_details['relationships']['investments']['paging']['first_page_url'] + '?page={}%20DESC&items_per_page=100&user_key={}'.format(page_investments, api_key)
            
            try:
                response = urllib.request.urlopen(url, timeout=timeout_time_seconds)
            except:
                continue
            data = json.loads(response.read().decode())
            page_investments = page_investments+1
        
    
        
            ## Investment Details - Paging
            
            investment_paging_total_items.append(data['data']['paging']['total_items'])
            investment_paging_number_of_pages.append(data['data']['paging']['number_of_pages'])
            investment_paging_current_page.append(data['data']['paging']['current_page'])
            investment_paging_sort_order.append(data['data']['paging']['sort_order'])
            investment_paging_items_per_page.append(data['data']['paging']['items_per_page'])
            investment_paging_next_page_url.append(data['data']['paging']['next_page_url'])
            investment_paging_prev_page_url.append(data['data']['paging']['prev_page_url'])
            investment_paging_key_set_url.append(data['data']['paging']['key_set_url'])
            investment_paging_collection_url.append(data['data']['paging']['collection_url'])
            
            
            
            for key in data['data']['items']:
                
                investment = key
                
                ## Investment Details - Investments
                investment_investor.append(name)
                investment_investor_uuid.append(uuid) 
                investment_type.append(investment['type'])
                investment_uuid.append(investment['uuid'])
                investment_money_invested.append(investment['properties']['money_invested'])
                investment_money_invested_currency_code.append(investment['properties']['money_invested_currency_code'])
                investment_money_invested_usd.append(investment['properties']['money_invested_usd'])
                investment_is_lead_investor.append(investment['properties']['is_lead_investor'])
                
                ## Investment Details - Funding Round
                
                investment_round = key['relationships']['funding_round']
                investment_round_type.append(investment_round['type'])
                investment_round_uuid.append(investment_round['uuid'])
                investment_round_permalink.append(investment_round['properties']['permalink'])
                investment_round_api_path.append(investment_round['properties']['api_path'])
                investment_round_web_path.append(investment_round['properties']['web_path'])
                investment_round_api_url.append(investment_round['properties']['api_url'])
                investment_round_funding_type.append(investment_round['properties']['funding_type'])
                investment_round_series.append(investment_round['properties']['series'])
                investment_round_series_qualifier.append(investment_round['properties']['series_qualifier'])
                investment_round_announced_on.append(investment_round['properties']['announced_on'])
                investment_round_closed_on.append(investment_round['properties']['closed_on'])
                investment_round_money_raised.append(investment_round['properties']['money_raised'])
                investment_round_money_raised_currency_code.append(investment_round['properties']['money_raised_currency_code'])
                investment_round_money_raised_usd.append(investment_round['properties']['money_raised_usd'])
                investment_round_target_money_raised.append(investment_round['properties']['target_money_raised'])
                investment_round_target_money_raised_currency_code.append(investment_round['properties']['target_money_raised_currency_code'])
                investment_round_target_money_raised_usd.append(investment_round['properties']['target_money_raised_usd'])
                investment_round_pre_money_valuation.append(investment_round['properties']['pre_money_valuation'])
                investment_round_pre_money_valuation_currency_code.append(investment_round['properties']['pre_money_valuation_currency_code'])
                investment_round_pre_money_valuation_usd.append(investment_round['properties']['pre_money_valuation_usd'])
                
                
                #Investor details for each funding round from each funding round url            
                funding_url = investment_round_api_url[-1] + '?&user_key={}'.format(api_key)
                
                try:
                    response = urllib.request.urlopen(funding_url, timeout=timeout_time_seconds)
                except:
                    timeout_excep_investors.append(url)
                    continue                
                data_funding = json.loads(response.read().decode())
                
                for investment_round_investors in data_funding['data']['relationships']['investments']['items']:
                    # Company details for reference
                    investor_uuid_funding_investor.append(uuid)
                    investor_name_funding_investor.append(name)
                    # Funding details for reference
                    investment_round_type_investment_round_investor.append(investment_round['type'])
                    investment_round_uuid_investment_round_investor.append(investment_round['uuid'])
                    investment_round_permalink_investment_round_investor.append(investment_round['properties']['permalink'])
                    investment_round_api_path_investment_round_investor.append(investment_round['properties']['api_path'])
                    investment_round_web_path_investment_round_investor.append(investment_round['properties']['web_path'])
                    investment_round_api_url_investment_round_investor.append(investment_round['properties']['api_url'])
                    investment_round_investment_round_type_investment_round_investor.append(investment_round['properties']['funding_type'])
                    investment_round_series_investment_round_investor.append(investment_round['properties']['series'])
                    investment_round_series_qualifier_investment_round_investor.append(investment_round['properties']['series_qualifier'])
                    investment_round_announced_on_investment_round_investor.append(investment_round['properties']['announced_on'])
                    investment_round_announced_on_trust_code_investment_round_investor.append(investment_round['properties']['announced_on_trust_code'])
                    investment_round_closed_on_investment_round_investor.append(investment_round['properties']['closed_on'])
                    investment_round_closed_on_trust_code_investment_round_investor.append(investment_round['properties']['closed_on_trust_code'])
                    investment_round_investment_round_money_raised_investment_round_investor.append(investment_round['properties']['money_raised'])
                    investment_round_money_raised_currency_code_investment_round_investor.append(investment_round['properties']['money_raised_currency_code'])
                    investment_round_money_raised_usd_investment_round_investor.append(investment_round['properties']['money_raised_usd'])
                    investment_round_target_money_raised_investment_round_investor.append(investment_round['properties']['target_money_raised'])
                    investment_round_target_money_raised_currency_code_investment_round_investor.append(investment_round['properties']['target_money_raised_currency_code'])
                    investment_round_target_money_raised_usd_investment_round_investor.append(investment_round['properties']['target_money_raised_usd'])
