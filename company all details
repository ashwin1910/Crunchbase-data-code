                        #except:
                            #time_excep_current_team_investment.append(url)
                            #continue
                        data_current_member_investments = json.loads(response.read().decode())
                        page = page+1
                        for current_member_investments in data_current_member_investments['data']['items']:
                            
                            company_uuid_current_member_investments.append(company_details['uuid'])
                            company_name_current_member_investments.append(company_details['properties']['name'])
                            current_member_uuid_current_member_investments.append(current_team_details['relationships']['person']['uuid'])
