                        member_advisory_role_organization_rank.append(member_advisory_roles['relationships']['organization']['properties']['rank'])
                        member_advisory_role_organization_created_at.append(member_advisory_roles['relationships']['organization']['properties']['created_at'])
                        member_advisory_role_organization_updated_at.append(member_advisory_roles['relationships']['organization']['properties']['updated_at'])
                        
                else: # Else append as missing entries
                    company_uuid_member_advisory_role.append(company_details['uuid'])
                    company_name_member_advisory_role.append(company_details['properties']['name'])
                    member_uuid_member_advisory_role.append(members_details['relationships']['person']['uuid'])
                    member_first_name_member_advisory_role.append(members_details['relationships']['person']['properties']['first_name'])
                    member_last_name_member_advisory_role.append(members_details['relationships']['person']['properties']['last_name'])
                    member_advisory_role_type.append('')
                    member_advisory_role_organization_uuid.append('')
                    member_advisory_role_title.append('')
                    member_advisory_role_started_on.append('')
                    member_advisory_role_started_on_trust_code.append('')
                    member_advisory_role_ended_on.append('')
                    member_advisory_role_ended_on_trust_code.append('')
                    member_advisory_role_is_current.append('')
                    member_advisory_role_job_type.append('')
                    member_advisory_role_created_at.append('')
                    member_advisory_role_updated_at.append('')
                    member_advisory_role_organization_permalink.append('')
                    member_advisory_role_organization_permalink_aliases.append('')
                    member_advisory_role_organization_api_path.append('')
                    member_advisory_role_organization_web_path.append('')
                    member_advisory_role_organization_api_url.append('')
                    member_advisory_role_organization_name.append('')
                    member_advisory_role_organization_also_known_as.append('')
                    member_advisory_role_organization_short_description.append('')
                    member_advisory_role_organization_description.append('')
                    member_advisory_role_organization_profile_image_url.append('')
                    member_advisory_role_organization_primary_role.append('')
                    member_advisory_role_organization_role_company.append('')
                    member_advisory_role_organization_role_investor.append('')
                    member_advisory_role_organization_role_group.append('')
                    member_advisory_role_organization_role_school.append('')
                    member_advisory_role_organization_investor_type.append('')
                    member_advisory_role_organization_founded_on.append('')
                    member_advisory_role_organization_founded_on_trust_code.append('')
                    member_advisory_role_organization_is_closed.append('')
                    member_advisory_role_organization_closed_on.append('')
                    member_advisory_role_organization_closed_on_trust_code.append('')
                    member_advisory_role_organization_num_employees_min.append('')
                    member_advisory_role_organization_num_employees_max.append('')
                    member_advisory_role_organization_stock_exchange.append('')
                    member_advisory_role_organization_stock_symbol.append('')
                    member_advisory_role_organization_total_funding_usd.append('')
                    member_advisory_role_organization_number_of_investments.append('')
                    member_advisory_role_organization_homepage_url.append('')
                    member_advisory_role_organization_contact_email.append('')
                    member_advisory_role_organization_phone_number.append('')
                    member_advisory_role_organization_rank.append('')
                    member_advisory_role_organization_created_at.append('')
                    member_advisory_role_organization_updated_at.append('')
                
                # All investments made by each member
                member_investments_paging_total_items.append(member_details['investments']['paging']['total_items'])
                if member_investments_paging_total_items[-1]>0: # If member website details are available, append the details
                    page=1
                    
                    member_investments_url = member_details['investments']['paging']['first_page_url']+'?user_key={}'.format(api_key)
                    response = urllib.request.urlopen(member_investments_url, timeout=timeout_time_seconds)
                    data_member_investments = json.loads(response.read().decode())
                    member_investments_paging_number_of_pages= data_member_investments['data']['paging']['number_of_pages']

                    
                    while page <= member_investments_paging_number_of_pages: # Pagination - For any item, CrunchBase API shows at most 100 entries per page. For items with over 100 entries, we have to scrape details from each page
                        member_investments_url = member_details['investments']['paging']['first_page_url'] + '?page={}&sort_order=announced_on%20DESC&items_per_page=100&user_key={}'.format(page, api_key)
                        
                        #try:
                        response = urllib.request.urlopen(member_investments_url, timeout=timeout_time_seconds)
                        #except:
                            #time_excep_members_investment.append(url)
                            #continue
                        data_member_investments = json.loads(response.read().decode())
                        page = page+1
                        for member_investments in data_member_investments['data']['items']:
                            
                            company_uuid_member_investments.append(company_details['uuid'])
                            company_name_member_investments.append(company_details['properties']['name'])
                            member_uuid_member_investments.append(members_details['relationships']['person']['uuid'])
                            member_first_name_investments.append(members_details['relationships']['person']['properties']['first_name'])
                            member_last_name_member_investments.append(members_details['relationships']['person']['properties']['last_name'])
                            member_investment_type.append(member_investments['type'])
                            member_investment_uuid.append(member_investments['uuid'])
                            member_investment_money_invested.append(member_investments['properties']['money_invested'])
                            member_investment_money_invested_currency_code.append(member_investments['properties']['money_invested_currency_code'])
                            member_investment_money_invested_usd.append(member_investments['properties']['money_invested_usd'])
                            member_investment_is_lead_investor.append(member_investments['properties']['is_lead_investor'])
                            member_investment_announced_on.append(member_investments['properties']['announced_on'])
                            member_investment_announced_on_trust_code.append(member_investments['properties']['announced_on_trust_code'])
                            member_investment_created_at.append(member_investments['properties']['created_at'])
                            member_investment_updated_at.append(member_investments['properties']['updated_at'])
                            member_investment_funding_round_type.append(member_investments['relationships']['funding_round']['type'])
                            member_investment_funding_round_uuid.append(member_investments['relationships']['funding_round']['uuid'])
                            member_investment_funding_round_permalink.append(member_investments['relationships']['funding_round']['properties']['permalink'])
                            member_investment_funding_round_api_path.append(member_investments['relationships']['funding_round']['properties']['api_path'])
                            member_investment_funding_round_web_path.append(member_investments['relationships']['funding_round']['properties']['web_path'])
                            member_investment_funding_round_api_url.append(member_investments['relationships']['funding_round']['properties']['api_url'])
                            member_investment_funding_round_funding_type.append(member_investments['relationships']['funding_round']['properties']['funding_type'])
                            member_investment_funding_round_series.append(member_investments['relationships']['funding_round']['properties']['series'])
                            member_investment_funding_round_series_qualifier.append(member_investments['relationships']['funding_round']['properties']['series_qualifier'])
                            member_investment_funding_round_announced_on.append(member_investments['relationships']['funding_round']['properties']['announced_on'])
                            member_investment_funding_round_announced_on_trust_code.append(member_investments['relationships']['funding_round']['properties']['announced_on_trust_code'])
                            member_investment_funding_round_closed_on.append(member_investments['relationships']['funding_round']['properties']['closed_on'])
                            member_investment_funding_round_closed_on_trust_code.append(member_investments['relationships']['funding_round']['properties']['closed_on_trust_code'])
                            member_investment_funding_money_raised.append(member_investments['relationships']['funding_round']['properties']['money_raised'])
                            member_investment_funding_round_money_raised_currency_code.append(member_investments['relationships']['funding_round']['properties']['money_raised_currency_code'])
                            member_investment_funding_round_money_raised_usd.append(member_investments['relationships']['funding_round']['properties']['money_raised_usd'])
                            member_investment_funding_round_target_money_raised.append(member_investments['relationships']['funding_round']['properties']['target_money_raised'])
                            member_investment_funding_round_target_money_raised_currency_code.append(member_investments['relationships']['funding_round']['properties']['target_money_raised_currency_code'])
                            member_investment_funding_round_target_money_raised_usd.append(member_investments['relationships']['funding_round']['properties']['target_money_raised_usd'])
                            member_investment_funding_round_pre_money_valuation.append(member_investments['relationships']['funding_round']['properties']['pre_money_valuation'])
