                            current_member_investment_funding_round_permalink.append(current_member_investments['relationships']['funding_round']['properties']['permalink'])
                            current_member_investment_funding_round_api_path.append(current_member_investments['relationships']['funding_round']['properties']['api_path'])
                            current_member_investment_funding_round_web_path.append(current_member_investments['relationships']['funding_round']['properties']['web_path'])
                            current_member_investment_funding_round_api_url.append(current_member_investments['relationships']['funding_round']['properties']['api_url'])
                            current_member_investment_funding_round_funding_type.append(current_member_investments['relationships']['funding_round']['properties']['funding_type'])
                            current_member_investment_funding_round_series.append(current_member_investments['relationships']['funding_round']['properties']['series'])
                            current_member_investment_funding_round_series_qualifier.append(current_member_investments['relationships']['funding_round']['properties']['series_qualifier'])
                            current_member_investment_funding_round_announced_on.append(current_member_investments['relationships']['funding_round']['properties']['announced_on'])
                            current_member_investment_funding_round_announced_on_trust_code.append(current_member_investments['relationships']['funding_round']['properties']['announced_on_trust_code'])
                            current_member_investment_funding_round_closed_on.append(current_member_investments['relationships']['funding_round']['properties']['closed_on'])
