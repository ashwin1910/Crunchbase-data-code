                            current_member_investment_funding_round_funded_organization_updated_at.append(current_member_investments['relationships']['funding_round']['relationships']['funded_organization']['properties']['updated_at'])           
        
        #extracting details of past team and members
        past_team_url = company_details['relationships']['past_team']['paging']\
        ['first_page_url'] +'?user_key={}'.format(api_key)
        #company_founders_url.append(founders_url)
        
        #try:
        response = urllib.request.urlopen(past_team_url, timeout=timeout_time_seconds)
        #except:
            #time_excep_company_founders.append(url)
            #continue
        data_past_team = json.loads(response.read().decode())
        past_team_paging_number_of_pages= data_past_team['data']['paging']['number_of_pages']
        page_past_team=1
        while page_past_team <= past_team_paging_number_of_pages: # Pagination - For any item, CrunchBase API shows at most 100 entries per page. For items with over 100 entries, we have to scrape details from each page
            past_team_url = company_details['relationships']['past_team']['paging']['first_page_url'] + '?page={}%20DESC&items_per_page=100&user_key={}'.format(page_past_team, api_key)
            
            #try:
            response = urllib.request.urlopen(past_team_url, timeout=timeout_time_seconds)
            #except:
                #time_excep_company_founder.append(url)
                #continue
            data_past_team = json.loads(response.read().decode())
            page_past_team = page_past_team+1        
            for past_team_details in data_past_team['data']['items']:
                company_uuid_past_member.append(company_details['uuid'])
                company_name_past_member.append(company_details['properties']['name'])
                past_member_type.append(past_team_details['type'])
                past_member_uuid.append(past_team_details['relationships']['person']['uuid'])
                past_member_permalink.append(past_team_details['relationships']['person']['properties']['permalink'])
                past_member_api_path.append(past_team_details['relationships']['person']['properties']['api_path'])
                past_member_web_path.append(past_team_details['relationships']['person']['properties']['web_path'])
                past_member_api_url.append(past_team_details['relationships']['person']['properties']['api_url'])
                past_member_first_name.append(past_team_details['relationships']['person']['properties']['first_name'])
                past_member_last_name.append(past_team_details['relationships']['person']['properties']['last_name'])
                past_member_gender.append(past_team_details['relationships']['person']['properties']['gender'])
                past_member_also_known_as.append(past_team_details['relationships']['person']['properties']['also_known_as'])
                past_member_bio.append(past_team_details['relationships']['person']['properties']['bio'])
                past_member_profile_image_url.append(past_team_details['relationships']['person']['properties']['profile_image_url'])
                past_member_role_investor.append(past_team_details['relationships']['person']['properties']['role_investor'])
                past_member_born_on.append(past_team_details['relationships']['person']['properties']['born_on'])
                past_member_born_on_trust_code.append(past_team_details['relationships']['person']['properties']['born_on_trust_code'])
                past_member_died_on.append(past_team_details['relationships']['person']['properties']['died_on'])
                past_member_died_on_trust_code.append(past_team_details['relationships']['person']['properties']['died_on_trust_code'])
                past_member_rank.append(past_team_details['relationships']['person']['properties']['rank'])
                past_member_created_at.append(past_team_details['relationships']['person']['properties']['created_at'])
                past_member_updated_at.append(past_team_details['relationships']['person']['properties']['updated_at'])    
    
                past_member_url= 'https://api.crunchbase.com/v3.1/people/{}?user_key={}'.format(past_team_details['relationships']['person']['uuid'], api_key)
                
                #try:
                response = urllib.request.urlopen(past_member_url, timeout=timeout_time_seconds)
                #except:
                    #timeout_excep_past_member_details.append(url)
                    #continue
                data_past_member = json.loads(response.read().decode())
                past_member_details = data_past_member['data']['relationships']
