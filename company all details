                        investment_round_investor_num_employees_max.append(investment_round_investors['relationships']['investors']['properties']['num_employees_max'])
                        investment_round_investor_stock_exchange.append(investment_round_investors['relationships']['investors']['properties']['stock_exchange'])
                        investment_round_investor_stock_symbol.append(investment_round_investors['relationships']['investors']['properties']['stock_symbol'])
                        investment_round_investor_total_investment_round_usd.append(investment_round_investors['relationships']['investors']['properties']['total_funding_usd'])
                        investment_round_investor_number_of_investments.append(investment_round_investors['relationships']['investors']['properties']['number_of_investments'])
                        investment_round_investor_homepage_url.append(investment_round_investors['relationships']['investors']['properties']['homepage_url'])
                        investment_round_investor_contact_email.append(investment_round_investors['relationships']['investors']['properties']['contact_email'])
                        investment_round_investor_phone_number.append(investment_round_investors['relationships']['investors']['properties']['phone_number'])
                        investment_round_investor_first_name.append('')
