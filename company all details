                if current_member_investments_paging_total_items[-1]>0: # If current_member website details are available, append the details
                    page=1
                    
                    current_member_investments_url = current_member_details['investments']['paging']['first_page_url']+'?user_key={}'.format(api_key)
                    response = urllib.request.urlopen(current_member_investments_url, timeout=timeout_time_seconds)
                    data_current_member_investments = json.loads(response.read().decode())
                    current_member_investments_paging_number_of_pages= data_current_member_investments['data']['paging']['number_of_pages']
                    
                    
                    while page <= current_member_investments_paging_number_of_pages: # Pagination - For any item, CrunchBase API shows at most 100 entries per page. For items with over 100 entries, we have to scrape details from each page
                        current_member_investments_url = current_member_details['investments']['paging']['first_page_url'] + '?page={}&sort_order=announced_on%20DESC&items_per_page=100&user_key={}'.format(page, api_key)
                        
                        #try:
