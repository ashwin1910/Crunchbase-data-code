        # All investments made by each person
        person_investments_paging_total_items.append(person_details['investments']['paging']['total_items'])
        if person_investments_paging_total_items[-1]>0: # If person website details are available, append the details
            page=1
            
            person_investments_url = person_details['investments']['paging']['first_page_url']+'?user_key={}'.format(api_key)
            response = urllib.request.urlopen(person_investments_url, timeout=timeout_time_seconds)
            data_person_investments = json.loads(response.read().decode())
            person_investments_paging_number_of_pages= data_person_investments['data']['paging']['number_of_pages']
        
            while page <= person_investments_paging_number_of_pages: # Pagination - For any item, CrunchBase API shows at most 100 entries per page. For items with over 100 entries, we have to scrape details from each page
                person_investments_url = person_details['investments']['paging']['first_page_url'] + '?page={}&sort_order=announced_on%20DESC&items_per_page=100&user_key={}'.format(page, api_key)
                
                
                response = urllib.request.urlopen(person_investments_url, timeout=timeout_time_seconds)
                data_person_investments = json.loads(response.read().decode())
                page = page+1
                for person_investments in data_person_investments['data']['items']:
                    
